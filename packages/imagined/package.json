{
  "name": "imagined",
  "version": "0.3.0",
  "description": "A build-time macro system for AI image generation that transforms React components into static images using the Recraft API",
  "keywords": [
    "ai",
    "image-generation",
    "recraft",
    "react",
    "macro",
    "build-time",
    "static-images"
  ],
  "license": "MIT",
  "author": "tone-row",
  "repository": {
    "type": "git",
    "url": "https://github.com/tone-row/imagined.git"
  },
  "homepage": "https://github.com/tone-row/imagined#readme",
  "bugs": {
    "url": "https://github.com/tone-row/imagined/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "imagined": "./dist/macro.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "src",
    "dist",
    "tsconfig.json"
  ],
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/bun": "latest",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  }
}
