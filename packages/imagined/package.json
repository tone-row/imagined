{
  "name": "imagined",
  "version": "0.1.0",
  "description": "A build-time macro system for AI image generation that transforms React components into static images using the Recraft API",
  "keywords": [
    "ai",
    "image-generation",
    "recraft",
    "react",
    "macro",
    "build-time",
    "static-images"
  ],
  "license": "MIT",
  "author": "tone-row",
  "repository": {
    "type": "git",
    "url": "https://github.com/tone-row/imagined.git"
  },
  "homepage": "https://github.com/tone-row/imagined#readme",
  "bugs": {
    "url": "https://github.com/tone-row/imagined/issues"
  },
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "imagined": "./src/macro.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./macro": {
      "types": "./src/macro.ts",
      "import": "./src/macro.ts"
    }
  },
  "files": [
    "src",
    "tsconfig.json"
  ],
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  }
}
